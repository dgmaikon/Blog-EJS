
<%- include("partials/header")-%>

<h1>Compose</h1>

<form action="/compose" method="post">
    <div class="form-group">
        <label for="inputTitle">Title</label>
        <input type="text" class="form-control" name="inputTitle" id="inputTitle">
    </div>

    <div class="form-group">
        <label for="inputPost">Post</label>
        <textarea class="form-control" name="inputPost" id="inputPost" rows="5"></textarea>  
    </div>

    <button type="submit" class="btn btn-primary btn-md active">Publish</button>

  </form>




<%- include("partials/footer")-%>